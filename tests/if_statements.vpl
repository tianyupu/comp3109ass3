func basic(a)
  if a < 20 then
    a = 20;
  else
  endif
end

func redundant(a)
  if a < 20 then
    a = 5;
  else
    a = 1;
  endif;
  a = 10;
end

func nested(a,b)
  if a < 15 then
    a = 5;
    if b < 5 then
      b = 10;
    else
      b = 7;
    endif
  else
    a = 3;
  endif
end

func twoIf(a,b)
  if a < 10 then
    a = 5
  else
    a = 10
  endif;
  if b < 20 then
    b = 10
  else
    b = 20
  endif
end

